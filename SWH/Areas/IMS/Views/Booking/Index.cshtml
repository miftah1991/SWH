        setCombo('#Comb_MID', '@Url.Action("GetMenu/")', 'MID', 'Name', 30, 200, 0);
        setCombo('#Comb_BTID', '@Url.Action("GetBookingType/")', 'BTID', 'BTName', 30, 200, 0);
        $('#Comb_MID').bind('select', function (event) {
