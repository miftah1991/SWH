        setInputDr("BookingRemarks", 500, 50);

        $('#CLActualGuest1,#CLServiceCharge1,#Tax').keyup(function () {
            booked = $("#CLBookedGuest1").val();
            actual = $("#CLActualGuest1").val();
            mprice = $("#CLMenuPrice1").val();
            servicecharge = $("#CLServiceCharge1").val();
            taxamount = $("#Tax").val();
            gprice = (Number(booked) + Number(actual)) * Number(mprice);
            $("#CLGuestPrice1").val(gprice);
            //alert(gprice);
            serviceamount = (Number(servicecharge) * Number(gprice)) / 100
            $("#CLServiceChargeAmount1").val(serviceamount);
            //discount = $("#CLDiscount").val();
            total = Number(gprice) + Number(serviceamount)+Number(taxamount) //- Number(discount);
            $("#CLTotalPrice1").val(total);
            prepaid = $("#CLPaymentAmount1").val();
            $("#CLBalance1").val(Number(total)-Number(prepaid));
            //alert(mprice)
        });
        $('#CLDiscount').keyup(function () {
            booked = $("#CLBookedGuest").val();
            actual = $("#CLActualGuest").val();
            mprice = $("#CLMenuPrice").val();
            //servicecharge = $("#CLServiceCharge1").val();
            gprice = (Number(booked) + Number(actual)) * Number(mprice);
            //$("#CLGuestPrice1").val(gprice);
            //alert(gprice);
            //serviceamount = (Number(servicecharge) * Number(gprice)) / 100
            serviceChargeAmount = $("#CLServiceChargeAmount").val();
            prepaid = $("#CLPaymentAmount").val();
            taxamount = $("#Tax1").val();
            totwithservicepre = ((gprice + Number(serviceChargeAmount) + Number(taxamount)) - Number(prepaid));
            discount = $("#CLDiscount").val();
            //total = Number(gprice) + Number(serviceamount) - Number(discount);
            //$("#CLTotalPrice").val(total);

            //$("#CLBalance").val(Number(total) - Number(prepaid));
            //var balance = $("#CLBalance").val();
            //var discount = $("#CLDiscount").val();
            $("#CLBalance").val(Number(totwithservicepre) - Number(discount));
            //alert(mprice)
        });
        $('#FinalExchangeRate').keyup(function () {
            exchange =$('#FinalExchangeRate').val();
            final = $("#FinalAmount").val();
            $("#FinalPayment").val(Number(exchange) * Number(final));
        })
