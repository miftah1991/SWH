        makeCombHidden(["Comb_CancelAmountCID"], 'frmBookingCan');
        setInputsDr(["CLPaymentAmount", "CLMenuName", "CLMenuPrice", "CancelRemarks", "CancelReason", "CancelAmountExchange", "CLBookedGuest", "ExchangeRate", "RemarksPayment", "BDate", "BHName", "MenuPrice", "CustomerName", "CustomerMobile", "CustomerEmail", "BookedGuest", "BookingNumber", "PrePaidExchangeRate"], 200, 30);
